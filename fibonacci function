#include <iostream>
using namespace std;
int main()
{
	int a[3],n;
	a[0] = 1;
	a[1] = 1;
	cin >> n;
	if (n < 3)
	{
		if (n == 1)
			cout << a[0];
		else
			cout << a[1];
	}
	else
	{
		for (int i = 0; i < n - 2; i++)
		{
			a[2] = a[0] + a[1];
			a[0] = a[1];
			a[1] = a[2];
		}
		cout << a[2];
	}
}
