#include <iostream>
using namespace std;
int main()
{
	int k, i, b, c, d;

	cin >> k;
	for (i = 100; i <= 500; i++)
	{
		b = i % 10;							//取个位数
		c = ((i - b) /10)%10;				//取十位数
		d = (i - b - c * 10) / 100;			//取百位数
		if (k == b + c + d)
		{
			if (d == 0 && c > 0)
			{
				cout << c << b << endl;
			}
				
			else if (d == 0 && c == 0)
			{
				cout << b << endl;
			}
				
			else if (d > 0)
			{
				cout << i << endl;
			}

		}


	}


}
